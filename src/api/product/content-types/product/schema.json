{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "float"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "quantity": {
      "type": "biginteger"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "M",
        "F"
      ]
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "S",
        "M",
        "L",
        "XL",
        "XXL"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "withinEnugu": {
      "type": "float"
    },
    "outsideEnugu": {
      "type": "float"
    },
    "internationlOrder": {
      "type": "float"
    }
  }
}
